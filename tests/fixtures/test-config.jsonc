  "base": {
    "NODE_ENV": "development",
    "APP_NAME": "je-test-app",
    "DEBUG": "true",
    "COMPLEX_VALUE": "value with spaces"
  },
  
  // Development specific overrides
  "development": {
    "API_URL": "http://localhost:3000",
